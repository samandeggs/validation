{
  "name": "samandeggs/validation",
  "description": "PHP Laravel like standalone validation library - based on Rakit/Validation",
  "license": "MIT",
  "authors": [
    {
      "email": "sam@infused.io",
      "name": "sam mulliss"
    }
  ],
  "autoload": {
    "psr-4": {
      "Rakit\\Validation\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Rakit\\Validation\\Tests\\": [
        "tests",
        "tests/Fixtures"
      ]
    }
  },
  "require": {
    "php": ">=7.0",
    "ext-mbstring": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.5",
    "squizlabs/php_codesniffer": "^3",
    "php-coveralls/php-coveralls": "^2.2"
  },
  "scripts": {
    "test": [
      "phpunit",
      "phpcs"
    ]
  }
}
